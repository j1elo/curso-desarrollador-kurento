# <#file>

all: make_demo

run: make_demo
	LD_LIBRARY_PATH=. ./make_demo

make_demo: make_demo_main.c libmake_calc.so
	gcc -o make_demo make_demo_main.c -L. -lmake_calc

libmake_calc.so: make_demo_calc.c
	gcc -shared -fPIC -o libmake_calc.so make_demo_calc.c -lm

clean:
	rm -f make_demo libmake_calc.so
